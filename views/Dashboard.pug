extends layout

block content
  link(rel="stylesheet", href="/assests/css/dashboard.css")
  .table-container
    h2 Registered Users
    if users && users.length > 0
      table#userTable
        thead
          tr
            th Username
            th Email
        tbody
          each user in users
            tr
              td= user.username
              td= user.email
    else
      p No registered users found.

  script(src="/assests/js/dashboard.js")
